const prompt = require ("prompt-sync")()                 // adiciona pacote prompt-sync para poder ser executado no cmd
const pessoas =  Number(prompt("N° Pessas: "))            // lê dados de entrada
const peixes = Number(prompt("N° Peixes extras: "))       // lê dados de entrada
let pagar                                                 // declara variável pagar
if (peixes <= pessoas) {                                  //concição definida no exemplo
    pagar = pessoas * 20
} else {
    pagar = (pessoas * 20) + ((peixes - pessoas) * 12)
}
console.log(`Pagar R$: ${pagar.toFixed(2)}`) 